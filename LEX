0S1|1S0|0|1

%{
#include "palindrome.tab.h"
%}
%%
[0]+ { return ZERO; }
[1]+ { return ONE; }
\n { return NL ;}
%%
 
 SAVE AS palindrome.l
 
 
--------- Yacc PART-----------
 
 %{
        #include<stdio.h>
        #include<stdlib.h>
        extern int yylex();
%}
%token ZERO ONE NL
%%
stmt : exp NL { printf("Valid Expression"); exit(0);}
;
exp : ZERO sm
| ONE am
;
sm : sm ONE
|
;
am : am ZERO
|
;
%%
int yyerror(char *msg)
{
printf("Invalid Expression\n");
exit(0);
}
int main ()
{
printf("Enter the expression\n");
yyparse();
return 0;
}
int yywrap()
{
        return 1;
}

/* odd even program */
%{
  #include<stdio.h>
  int x;
%}
%%
[0-9]+ {x=atoi (yytext);
        if(x%2==0)
           printf("even number");
        else
           printf("odd number");
}
.* {printf("input not a  number");}
%%
int main()
{
printf("enter the input\n");
yylex();
return 1;
}
int yywrap()
{
return 1;
}


//function arguments

%{
  #include<stdio.h>
  int actualzero[30], zero, one, two;
%}
%%
[a-zA-Z_]+[0-9]*"();" {zero++;}
[a-zA-Z_]+[0-9]*"("[ a-zA-Z0-9\.']+");" {one++;}
[a-zA-Z_]+[0-9]*"("[ a-zA-Z0-9\.']+[,][ a-zA-Z0-9\.']+");" {two++;}
.* {;}
%%
int main()
{
yyin = fopen("arguments.c", "r");
yylex();
printf("zeros arguments= %d, ones arguments = %d, twos arguments = %d", zero, one, two);
return 1;
}
int yywrap()
{
return 1;
}

c file code
#include<stdio.h>
main()
{
printf();
scanf();
m();
ma(1.5);
mb(1, 'a');
}

